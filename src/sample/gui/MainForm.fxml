<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.MainFormController">
   <children>
      <MenuBar>
        <menus>
          <Menu mnemonicParsing="false" text="Файл">
               <items>
                  <MenuItem mnemonicParsing="false" text="Сохранить" onAction="#onSaveToFileClick" />
                  <MenuItem mnemonicParsing="false" text="Загрузить" onAction="#onLoadToFileClick" />
               </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Данные">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#onAddClick" text="Добавить" />
                  <MenuItem mnemonicParsing="false" onAction="#onEditClick" text="Редактировать" />
                  <MenuItem mnemonicParsing="false" onAction="#onDeleteClick" text="Удалить" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <TableView fx:id="mainTable" prefHeight="458.0" prefWidth="482.0" />
   </children>
</VBox>
