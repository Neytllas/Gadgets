<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.gui.MainFormController">
   <children>
      <HBox prefHeight="0.0" prefWidth="482.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onAddClick" text="Добавить">
               <HBox.margin>
                  <Insets right="50.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#onEditClick" prefHeight="31.0" prefWidth="147.0" text="Редактировать" />
         </children>
         <padding>
            <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
         </padding>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <TableView fx:id="mainTable" prefHeight="458.0" prefWidth="482.0" />
   </children>
</VBox>
